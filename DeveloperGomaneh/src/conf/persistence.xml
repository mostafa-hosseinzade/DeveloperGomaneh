<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="DeveloperPU" transaction-type="JTA">
    <jta-data-source>jdbc/myDeDatasource</jta-data-source>
    <class>Entity.Contact</class>
    <class>Entity.Content</class>
    <class>Entity.ContentCategory</class>
    <class>Entity.Factor</class>
    <class>Entity.FactorField</class>
    <class>Entity.Faq</class>
    <class>Entity.Portfolio</class>
    <class>Entity.PortfolioCategory</class>
    <class>Entity.ProtfolioImg</class>
    <class>Entity.SectionFactor</class>
    <class>Entity.Seo</class>
    <class>Entity.User</class>
    <class>Entity.Order</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <validation-mode>NONE</validation-mode>
    <properties>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
